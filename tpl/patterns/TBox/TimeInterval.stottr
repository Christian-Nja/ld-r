@prefix odp-tpl:    <http://www.ontologydesignpatterns.org/tpl#> .
@prefix odp-pat:    <http://www.ontologydesignpatterns.org/cp/owl/> .
@prefix owl:        <http://www.w3.org/2002/07/owl#> . 
@prefix o-rdfs:     <http://tpl.ottr.xyz/rdfs/0.2/> .
@prefix o-owl-ax:   <http://tpl.ottr.xyz/owl/axiom/0.1/> .
@prefix odp-ti:     <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#> .


odp-tpl:TimeInterval[   owl:Class ?timeInterval=odp-ti:TimeInterval, 
                        owl:DatatypeProperty ?hasIntervalDate=odp-ti:hasIntervalDate, 
                        owl:DatatypeProperty ?hasIntervalEndDate=odp-ti:hasIntervalEndDate, 
                        owl:DatatypeProperty ?hasIntervalStartDate=odp-ti:hasIntervalStartDate  ] :: {

    odp-tpl:Pattern(odp-pat:time-interval),
    o-rdfs:Domain(?hasIntervalDate, ?timeInterval),
    o-owl-ax:SubDataPropertyOf(?hasIntervalEndDate, ?hasIntervalDate),
    o-owl-ax:SubDataPropertyOf(?hasIntervalStartDate, ?hasIntervalDate)
} .